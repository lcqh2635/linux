{
  "name": "unocss-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A project built with Bun",
  "author": "Your Name <you@example.com>",
  "license": "MIT",
  "scripts": {
    "dev": "bunx --bun vite --mode .env.development",
    "prod": "bunx --bun vite --mode .env.production",
    "test": "bunx --bun vitest --mode .env.test",
    "build": "bun vue-tsc -b && bun vite build",
    "build:staging": "vite build --mode staging",
    "preview": "bun vite preview",
    "format": "prettier --write .",
    "preinstall": "npx only-allow bun",
    "postinstall": "echo '依赖包下载完成...'"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true",
      "biome check --write --no-errors-on-unmatched",
      "biome check --write --organize-imports-enabled=false --no-errors-on-unmatched",
      "biome check --write --unsafe --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched",
      "biome lint --write --no-errors-on-unmatched"
    ],
    "*": [
      "biome check --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "dependencies": {
    "axios": "^1.9.0",
    "echarts": "^5.6.0",
    "figlet": "^1.8.1",
    "gsap": "^3.13.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "screenfull": "^6.0.2",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@types/bun": "^1.2.12",
    "@types/figlet": "^1.7.0",
    "@types/nprogress": "^0.2.3",
    "@unocss/reset": "^66.1.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/ui": "^3.1.3",
    "@vue/tsconfig": "^0.7.0",
    "postcss": "^8.5.3",
    "postcss-preset-env": "^10.1.6",
    "sass": "^1.87.0",
    "typescript": "~5.8.3",
    "unocss": "^66.1.1",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "bun": ">=1.1.20",
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bunred/bun-vue3-ts.git"
  },
  "bugs": {
    "url": "https://github.com/bunred/bun-vue3-ts/issues"
  },
  "homepage": "https://github.com/bunred/bun-vue3-ts#readme",
  "keywords": [
    "vue3",
    "typescript",
    "bun"
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
