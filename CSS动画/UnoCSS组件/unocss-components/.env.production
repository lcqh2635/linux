# 生产环境，生产环境专有配置

# 线上真实 API 地址
VITE_API_BASE_URL=https://api.yourdomain.com

# 禁用 Mock（生产环境不使用）
VITE_USE_MOCK=false

# 是否启用性能监控或埋点（如 Sentry、Datadog、友盟等）
VITE_ENABLE_MONITOR=true

# 第三方统计 ID（仅限生产使用）
VITE_ANALYTICS_ID=ga-123456789

# 是否开启调试日志（生产环境一般关闭）
VITE_ENABLE_DEBUG_LOG=false

# CDN 静态资源地址（可选）
VITE_CDN_URL=https://cdn.yourdomain.com/assets/

# 多语言国际化打包策略（是否压缩语言包）
VITE_I18N_COMPRESSION=false

# 构建版本号（可配合 CI/CD 注入）
VITE_APP_VERSION=1.0.0